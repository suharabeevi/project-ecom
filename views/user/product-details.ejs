<script>
    function Addtocart(proId) {
        $.ajax({
            url: '/Usercart/' + proId,
            method: 'get',
            data: proId,
            success: (response) => {
                if (response.outofstock) {

                    Swal.fire({
                        title: 'OUT OF STOCK',
                        text: '',
                        icon: 'danger',
                        timer: 5000
                    })
                } else {

                    Swal.fire({
                        title: 'Add To Cart',
                        text: 'Added to cart successfully',
                        icon: 'success',
                        timer: 5000
                    })

                }
            }
        })

    }

</script>